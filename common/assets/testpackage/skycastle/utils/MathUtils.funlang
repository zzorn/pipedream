
module MathUtils {

    fun lerp(a, b, t) = a * (1 - t) + b * t
    fun lerpVec2(a: Vec2, b: Vec2, t): Vec2 = a * (1 - t) + b * t
    fun lerpVec3(a: Vec3, b: Vec3, t): Vec3 = a * (1 - t) + b * t

    fun foo(p) = false

    fun many(f: Any => Any) {
       return [f(), f(), f()]
    }

